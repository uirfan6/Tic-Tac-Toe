pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building Your Pipleline'
            }
        }
	stage('Test'){
		steps {
			echo 'Testing your Pipeline'
		}
	}
	stage ('deploy'){
		steps {
			echo 'Deploying your Pipeline'
			echo 'All done UROOSA!!'
		}
	}
    }
}
